//compile c++ form commoand prompt: g++ -Wall this.cpp -o objectfile
#include <iostream>

using namespacestd;



int main(){

	int kn=0;
	int kp=1;
	int n=1000000;
	int a[n];
	a[0]=2;

	for(unsigned k=3;k=n-1;k++){

		while(a[kn]){		  	

			if((k%a[kn])==0){	
				break;
			}			             
			else(){			     
				if(a[kn]*a[kn]>k){
					a[kp]=k;	      
					kp++;
					break;
				}
			}
		}		
		kn++;
	}
	kn=0;
	
	return 0;
}
